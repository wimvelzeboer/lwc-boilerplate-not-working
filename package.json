{
  "name": "lwr-broken",
  "version": "0.0.1",
  "license": "MIT",
  "private": true,
  "type": "module",
  "scripts": {
    "clean": "rm -rf node_modules __lwr_cache__ site",
    "dev": "lwr dev",
    "dev:compat": "lwr dev --mode compat",
    "build": "lwr build --clean",
    "build:prod-compat": "lwr build --clean --mode prod-compat",
    "start": "lwr start",
    "stage": "yarn build && yarn start",
    "stage:prod-compat": "yarn build:prod-compat && yarn start"
  },
  "dependencies": {
    "lwr": "0.10.10"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "volta": {
    "node": "18.16.1"
  },
  "devDependencies": {
    "@lwc/module-resolver": "^2.50.0",
    "lwc": "^2.50.0",
    "lwc-webpack-plugin": "^3.0.4",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4"
  }
}
